{
    "compilerOptions": {
      "baseUrl": ".",
      "rootDir": "./",
      "module": "ES6",
      "outDir": "./dist/esm",
      "target": "ES2015",
      "lib": ["ESNext", "DOM"],
      "types": ["cypress", "node"],
      "declaration": false,
      "sourceMap": true,
      "strict": true,
      "noImplicitAny": false,
      "moduleResolution": "node",
      "declarationMap": false,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules", "dist"],
    "include": ["index.ts", "index.d.ts"]
  }
  